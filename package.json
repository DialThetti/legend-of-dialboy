{
  "name": "legend-of-dialboy",
  "version": "0.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "prod": "serve ./dist",
    "start": "webpack serve --config webpack.config.dev.js",
    "build": "webpack --config webpack.config.js ",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "format": "npx prettier --write 'src/**/*.ts'",
    "lint": "npx eslint --fix 'src/**/*.ts'",
    "dependencyInsight:svg": "node_modules/.bin/depcruise --ts-config tsconfig.json --exclude '^node_modules' --output-type dot src/app/main.ts > dependencies.dot && dot dependencies.dot -T svg -o dependencies.svg",
    "dependencyInsight:json": "node_modules/.bin/depcruise --ts-config tsconfig.json --exclude '^node_modules' --output-type json src/app/main.ts > dependencies.json"
  },
  "nyc": {
    "extension": [".ts"],
    "exclude": ["**/*.d.ts", "**/*.spec.ts"],
    "include": ["src/**"],
    "reporter": ["html", "clover", "lcovonly"],
    "all": true
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "babel-eslint": "^10.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "dependency-cruiser": "^13.0.4",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "46.2.6",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-mockito": "^2.6.1",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "^5.1.3",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {}
}
